{
    "name": "antonioprimera/laravel-site-components",
    "description": "Useful reusable components for laravel sites",
    "keywords": [
        "antonioprimera",
        "laravel",
        "laravel-site-components"
    ],
    "homepage": "https://github.com/antonioprimera/laravel-site-components",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Primera",
            "email": "antonio@cus.ro",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "antonioprimera/laravel-generator-command": "^3.0",
        "antonioprimera/laravel-site": "dev-master",
        "antonioprimera/md-parser": "^1.2",
        "illuminate/contracts": "^10.0||^11.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "AntonioPrimera\\SiteComponents\\": "src/",
            "AntonioPrimera\\SiteComponents\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntonioPrimera\\SiteComponents\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-site-components --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AntonioPrimera\\SiteComponents\\SiteComponentsServiceProvider"
            ],
            "aliases": {
                "SiteComponents": "AntonioPrimera\\SiteComponents\\Facades\\SiteComponents"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
