{
    "name": "antonioprimera/laravel-site-components",
    "description": "Useful reusable components for laravel sites",
    "keywords": [
        "antonioprimera",
        "laravel",
        "laravel-site-components"
    ],
    "homepage": "https://github.com/antonioprimera/laravel-site-components",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Primera",
            "email": "antonio@cus.ro",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": ">=11.0",
        "antonioprimera/laravel-site": "^v1.0",
        "antonioprimera/md-parser": "^1.2",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "AntonioPrimera\\SiteComponents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntonioPrimera\\SiteComponents\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AntonioPrimera\\SiteComponents\\SiteComponentsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
